<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<VBox style="-fx-background-color: #888;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.diogocapela.javafxboiler.controllers.MenuController">
    <children>
        <Button mnemonicParsing="false" onAction="#openDashboard" prefHeight="45.0" prefWidth="150.0" text="Dashboard"/>
        <Button mnemonicParsing="false" onAction="#openAreasRestritas" prefHeight="45.0" prefWidth="150.0" text="Áreas Restritas">
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#openColaboradores" prefHeight="45.0" prefWidth="150.0" text="Colaboradores">
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#openEquipamentos" prefHeight="45.0" prefWidth="150.0" text="Equipamentos">
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
        </Button>
        <Button mnemonicParsing="false" onAction="#openPerfisAutorizacao" prefHeight="45.0" prefWidth="150.0" text="Perfis de Autorização">
            <VBox.margin>
                <Insets top="10.0"/>
            </VBox.margin>
        </Button>
    </children>
    <padding>
        <Insets/>
    </padding>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>

